source "toolchain/Kconfig"

menu "Packages"

config SRC_BASEDIR
	string "Package sources base directory"
	default "/usr/src"

	help
	  The directory under which the package source trees will be
	  looked for. If relative, it is relative to Mkrootfs directory.

source "base/Kconfig"

source "linux/Kconfig"

endmenu
